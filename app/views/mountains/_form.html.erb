<p id="notice"><%= notice %></p>
<%= simple_form_for @mountain do |f| %>
  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :height %>
    <%= f.input :latitude %>
    <%= f.input :longitude %>
    <%= f.input :image %>
    <%= f.button :submit, "Add mountain" %>
  </div>
<% end %>
